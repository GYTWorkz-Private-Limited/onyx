# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_EMBEDDING_MODEL=text-embedding-3-small

# Qdrant Configuration
QDRANT_HOST=localhost
QDRANT_PORT=6333
QDRANT_TIMEOUT=60

# Indexing Configuration
CHUNK_SIZE=512
CHUNK_OVERLAP=100
BATCH_SIZE=50
